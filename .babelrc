{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["import", { 
      "libraryName": "antd", 
      "libraryDirectory": "lib",
      "style": true 
    }],
    "transform-es2015-destructuring",
    "transform-object-rest-spread"
  ]
}
